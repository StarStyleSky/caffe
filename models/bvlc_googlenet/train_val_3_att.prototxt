layer {
  name: "inception_3b/relu_pool_proj"
  type: "ReLU"
  bottom: "inception_3b/pool_proj"
  top: "inception_3b/pool_proj"
}
layer {
  name: "inception_3b/output"
  type: "Concat"
  bottom: "inception_3b/1x1"
  bottom: "inception_3b/3x3"
  bottom: "inception_3b/5x5"
  bottom: "inception_3b/pool_proj"
  top: "inception_3b/output"
}

#---------------------------------------------adding kernel attention--------------------------------------------------

layer {
  name: "inception_3a_att_conv1"
  type: "Convolution"
  bottom: "inception_3a/output"
  top: "inception_3a_att_conv1"
  param {
    lr_mult: 0.001
    decay_mult: 0
  }
  convolution_param {
    num_output: 256
    kernel_size: 3
	stride : 2
	pad :1
	bias_term: false
    weight_filler {
      type: "gaussian"
      std: 0.00001
    }
  }
  propagate_down: false
}
layer {
  name: "inception_3a_att_conv1_relu"
  type: "ReLU"
  bottom: "inception_3a_att_conv1"
  top: "inception_3a_att_conv1"
}
layer {
  name: "inception_3a_att_conv2"
  type: "Convolution"
  bottom: "inception_3a_att_conv1"
  top: "inception_3a_att_conv2"
  param {
    lr_mult: 0.001
    decay_mult: 0
  }
  convolution_param {
    num_output: 512
    kernel_size: 3
	stride : 2
	pad :1
	bias_term: false
    weight_filler {
      type: "gaussian"
      std: 0.00001
    }
  }
}
layer {
  name: "inception_3a_att_conv2_relu"
  type: "ReLU"
  bottom: "inception_3a_att_conv2"
  top: "inception_3a_att_conv2"
}
layer {
  name: "inception_3a_att_conv3"
  type: "Convolution"
  bottom: "inception_3a_att_conv2"
  top: "inception_3a_att_conv3"
  param {
    lr_mult: 0.001
    decay_mult: 0
  }
  convolution_param {
    num_output: 480
    kernel_size: 3
	stride : 2
	pad :1
    weight_filler {
      type: "gaussian"
      std: 0.00001
    }
	bias_term: false
  }
}
layer {
  name: "inception_3a_att_resize"
  type: "Resize"
  bottom: "inception_3a_att_conv3"
  top: "inception_3a_att_kernel"
  roi_pooling_param {
    pooled_w: 3
    pooled_h: 3
    spatial_scale: 1.0 # 1/16
  }
}
layer {
  name: 'inspect_3a_att_kernel'
  type: 'Python'
  bottom: 'inception_3a_att_kernel'
  top: 'inception_3a_att_kernel'
  python_param {
    module: 'inspect_layer'
    layer: 'InspectLayer'
	param_str:"{'string' : '3a_kernel'}"
  }
}

layer {
  name: 'inception_3a_att_kernel_bias'
  type: 'Python'
  bottom: 'inception_3a_att_kernel'
  top: 'inception_3a_att_kernel'
  python_param {
    module: 'kernel_bias_layer'
    layer: 'KernelBiasLayer'
  }
}

layer {
    bottom: "inception_3b/output"
    bottom:"inception_3a_att_kernel"
    top: "inception_attention_3b"
    name: "inception_attention_3b"
    type: "CrossConvolution"
    convolution_param {
        num_output: 480 # K
        kernel_size: 3
		group: 480
        stride : 1
        pad:1
    }
}
layer {
  name: "inception_attention_3b_relu"
  type: "ReLU"
  bottom: "inception_attention_3b"
  top: "inception_attention_3b"
}
#------------------------------------------------------------------------------------
layer {
  name: "pool3/3x3_s2"
  type: "Pooling"
  bottom: "inception_attention_3b"
  top: "pool3/3x3_s2"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
layer {
  name: "inception_4a/1x1"
  type: "Convolution"
  bottom: "pool3/3x3_s2"
  top: "inception_4a/1x1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 192
    kernel_size: 1
    weight_filler {
      type: "xavier"
      std: 0.03
    }
    bias_filler {
      type: "constant"
      value: 0.2
    }
  }
}
layer {
  name: "inception_4a/relu_1x1"
  type: "ReLU"
  bottom: "inception_4a/1x1"
  top: "inception_4a/1x1"
}
layer {
  name: "inception_4a/3x3_reduce"
  type: "Convolution"
